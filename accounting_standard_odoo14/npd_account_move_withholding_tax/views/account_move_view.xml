<?xml version="1.0" encoding="utf-8"?>
<odoo>

     <record id="view_move_withholding_form" model="ir.ui.view">
        <field name="name">account.move.withholding.form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form" />
        <field name="arch" type="xml">
           
            <page id="other_tab" position="before">
                <page
                    id="wht_tax_invoice"
                    string="Withholding Tax"
                >
                    <field name="wht_line_ids"  attrs="{'readonly': [('state','=','posted')]}">
                        <tree
                            string="Withholding Tax"
                            create="1"
                            delete="1"
                        >
                            <field name="income_tax_form" />
                                    <field name="name" />
                                    <field name="date" />
                                    <field name="supplier_partner_id" />
                                    <field name="state" />
                                    <field name="base_amount" />
                                    <field name="tax_amount" />
                        </tree>
                    </field>
                </page>
            </page>
        </field>
    </record>
</odoo>
