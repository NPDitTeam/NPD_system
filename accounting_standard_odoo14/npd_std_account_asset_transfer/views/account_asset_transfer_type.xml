<?xml version="1.0" encoding="utf-8" ?>
<odoo>


    <record id="account_asset_transfer_type" model="ir.actions.act_window">
        <field name="name">Asset Transfer Type</field>
        <field name="res_model">std.account.asset.transfer.type</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                Click to add a new Asset Transfer Type.
            </p>
        </field>
    </record>

        <record id="account_asset_transfer_type_view_tree" model="ir.ui.view">
        <field name="name">std.account.asset.transfer.type.tree</field>
        <field name="model">std.account.asset.transfer.type</field>
        <field name="arch" type="xml">
            <tree string="">
                <field name="code"/>
                <field name="name"/>
            </tree>
        </field>
    </record>

</odoo>
