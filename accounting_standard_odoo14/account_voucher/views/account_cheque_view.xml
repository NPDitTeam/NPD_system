<?xml version="1.0" encoding="utf-8"?>
<odoo>


  <record id="account_cheque_view_voucher_form" model="ir.ui.view">
    <field name="name">account.cheque.view.form.inherit</field>
    <field name="model">account.cheque</field>
            <field name="inherit_id" ref="account_cheque.account_cheque_view_form"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='payment_id']" position="after">
                    <field name="voucher_id"/>
                </xpath>

            </field>
        </record>



</odoo>