<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">

        <!-- Voucher-related subtypes for messaging / Chatter -->
        <record id="mt_voucher_state_change" model="mail.message.subtype">
            <field name="name">Status Change</field>
            <field name="res_model">account.voucher</field>
            <field name="default" eval="False"/>
            <field name="description">Status changed</field>
        </record>

        <record id="seq_sale_receipt" model="ir.sequence">
             <field name="name">Sale Receipt</field>
            <field name="code">sale.receipt</field>
            <field name="prefix">SA%(range_y)s%(month)s%(day)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
         <record id="seq_purchase_receipt" model="ir.sequence">
             <field name="name">Purchase Receipt</field>
            <field name="code">purchase.receipt</field>
            <field name="prefix">PA%(range_y)s%(month)s%(day)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
