<?xml version="1.0" encoding="utf-8"?>
<odoo>
      <record id="date_credit_card_done_view_form" model="ir.ui.view">
        <field name="name">date.credit.card.done.view.form</field>
        <field name="model">date.credit.card.done</field>
        <field name="arch" type="xml">
        <form string="Credit Card Done">
            <sheet>
            <group>
             <separator string="Date Receipt credit_card" colspan="4" />
                <group>
                    <field name="date_done" />
                </group>
            </group>
                    <footer>
                        <button string="Confirm" name="action_done_credit_card" type="object" class="oe_highlight"/>
                        <button string="Cancel" class="btn btn-secondary" special="cancel"/>
                    </footer>
            </sheet>
        </form>
        </field>
    </record>
    

</odoo>
