<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_customer_payment" model="ir.sequence">
            <field name="name">Customer Payment</field>
            <field name="code">customer.payment</field>
            <field name="prefix">CUSTIN-%(year)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
     <record id="seq_supplier_payment" model="ir.sequence">
            <field name="name">Supplier Payment</field>
            <field name="code">supplier.payment</field>
            <field name="prefix">SUPPOUT-%(year)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
