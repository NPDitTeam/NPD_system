<odoo>

    <!-- PDF REPORTS -->
    <!-- Tax Invoice-->
     <record id="action_report_acc_menu_print" model="ir.actions.report">
        <field name="name">______Accounting______</field>
        <field name="model">account.move</field>
        <field name="report_name">npd_std_accounting_qweb.report_acc_menu</field>
        <field name="report_file">npd_std_accounting_qweb.report_acc_menu</field>
        <field name="report_type">qweb-pdf</field>
        <field name="paperformat_id" ref="npd_std_accounting_qweb.paperformat_accounting_pdf"/>
        <field name="print_report_name">'%s%s' % (object.name, object.state == 'draft' and '-draft' or '')
        </field>
        <field name="binding_type">report</field>
    </record>

