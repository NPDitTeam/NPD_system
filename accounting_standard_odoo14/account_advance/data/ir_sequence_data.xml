<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_account_advance_request" model="ir.sequence">
            <field name="name">Advance Request</field>
            <field name="code">account.advance.request</field>
            <field name="prefix">ADR%(year)s%(month)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
       <record id="seq_account_advance" model="ir.sequence">
            <field name="name">Account Advance</field>
            <field name="code">account.advance</field>
            <field name="prefix">ADV%(year)s%(month)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_account_advance_clear" model="ir.sequence">
            <field name="name">Account Advance Clear</field>
            <field name="code">account.advance.clear</field>
            <field name="prefix">ADC%(year)s%(month)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
