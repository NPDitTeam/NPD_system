<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="action_report_stock_onhand_report_html" model="ir.actions.client">
        <field name="name">Stock Onhand Report</field>
        <field name="tag">stock_onhand_report_backend</field>
        <field name="context" eval="{'active_model': 'report.stock.onhand.report'}" />
    </record>

   <record id="action_stock_onhand_report_pdf" model="ir.actions.report">
        <field name="name">Stock Onhand PDF</field>
        <field name="model">report.stock.onhand.report</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">npd_stock_onhand.report_stock_onhand_report_pdf</field>
        <field name="report_file">npd_stock_onhand.report_stock_onhand_report_pdf</field>
        <field
            name="print_report_name"
        >'Stock Onhand Report - [%s]' % (object.location_id.complete_name)</field>
        <field name="paperformat_id" ref="npd_stock_onhand.paperformat_stock_onhand" />
    </record>

    <record id="action_stock_onhand_report_xlsx" model="ir.actions.report">
        <field name="name">Stock Onhand XLSX</field>
        <field name="model">report.stock.onhand.report</field>
        <field name="report_type">xlsx</field>
        <field
            name="report_name"
        >npd_stock_onhand.report_stock_onhand_report_xlsx</field>
        <field name="report_file">Stock Onhand Report</field>
    </record>

</odoo>
