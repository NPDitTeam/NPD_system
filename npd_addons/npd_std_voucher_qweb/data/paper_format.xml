<odoo>
    <record id="paperformat_voucher" model="report.paperformat">
        <field name="name">Voucher Form A4</field>
        <field name="default" eval="True" />
        <field name="format">A4</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">75</field>
        <field name="margin_bottom">38</field>
        <field name="margin_left">5</field>
        <field name="margin_right">5</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">70</field>
        <field name="dpi">90</field>
    </record>
<record id="paperformat_voucher_advance_clear" model="report.paperformat">
        <field name="name">Voucher Form A4 (advance clear)</field>
        <field name="default" eval="True"/>
        <field name="format">A4</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">75</field>
        <field name="margin_bottom">38</field>
        <field name="margin_left">5</field>
        <field name="margin_right">5</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">70</field>
        <field name="dpi">90</field>
</record>    
</odoo>
