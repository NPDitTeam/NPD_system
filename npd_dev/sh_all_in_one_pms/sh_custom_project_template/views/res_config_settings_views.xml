<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sh_default_project_stage_config_setting_view" model="ir.ui.view">
        <field name="name">project.stage.config.setting.view</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="project.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//div[@data-key='project']//div[1]" position="after">
                <h2>New Project Configuration</h2>
                <div class="row mt16 o_settings_container">
                    <div class="col-6 o_setting_box">
                        <div class="o_setting_right_pane">
                            <label for="sh_project_stage" />
                            <br />
                            <field name="sh_project_stage" class="oe_inline" />
                        </div>
                        <div class="o_setting_left_pane"></div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>
</odoo>