<?xml version="1.0" encoding="utf-8"?>
<odoo>
<template id="sh_report_producttemplatelabel">
    <t t-call="web.basic_layout">
        <div class="page">
            <t t-foreach="docs" t-as="template">
                <t t-foreach="template.product_variant_ids" t-as="product">
                    <t t-call="sh_product_qrcode_generator.sh_report_simple_label">
                        <t t-set="product" t-value="product"/>
                    </t>
                </t>
            </t>
        </div>
    </t>
</template>
<template id="sh_report_producttemplateqrcode">
    <t t-call="web.basic_layout">
        <div class="page">
            <t t-foreach="docs" t-as="template">
                <t t-foreach="template.product_variant_ids" t-as="product">
                    <t t-call="sh_product_qrcode_generator.sh_report_simple_qrcode">
                        <t t-set="product" t-value="product"/>
                    </t>
                </t>
            </t>
        </div>
    </t>
</template>
</odoo>
